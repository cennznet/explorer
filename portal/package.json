{
  "private": true,
  "name": "portal",
  "scripts": {
    "start": "node dist/main.js",
    "clean": "rimraf dist public",
    "build": "npm run clean && npm run build:node && npm run build:browser",
    "build:node": "tsc -p tsconfig.json",
    "build:browser": "webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=local nodemon",
    "lint": "npm run lint:ts & npm run format:ts",
    "lint:ts": "tslint --fix -t stylish -c tslint.json -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --collectCoverage",
    "format:ts": "prettier --parser typescript --write 'src/**/*.ts'",
    "migrate": "cross-env NODE_ENV=local ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "cypress": "cypress open",
    "ft": "cypress run"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.1",
    "@nestjs/core": "^5.4.1",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "5.7.3",
    "@types/socket.io": "2.1.2",
    "aws-sdk": "2.413.0",
    "axios": "^0.18.0",
    "bignumber.js": "8.1.1",
    "bootstrap-datepicker": "1.8.0",
    "bootstrap-select": "1.13.5",
    "cache-manager": "^2.9.0",
    "cache-manager-ioredis": "^1.0.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "clipboard": "2.0.4",
    "dayjs": "1.8.10",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-nunjucks": "^2.2.3",
    "fs-extra": "8.0.1",
    "i18n": "^0.8.3",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "nunjucks": "^3.1.4",
    "qrcode": "1.3.3",
    "rxjs": "^6.3.3",
    "slick-carousel": "1.8.1",
    "typeorm": "^0.2.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.6.0",
    "@nestjs/testing": "^5.4.1",
    "@types/cache-manager": "^1.2.6",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.11",
    "bootstrap": "^4.1.3",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cypress": "^3.4.1",
    "echarts": "^4.2.0-rc.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "raw-loader": "1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
