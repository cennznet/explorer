{
  "private": true,
  "name": "websocket",
  "scripts": {
    "start": "node dist/main.js",
    "clean": "rimraf dist public",
    "build": "npm run clean && npm run build:node",
    "build:node": "tsc -p tsconfig.json",
    "dev": "cross-env NODE_ENV=local nodemon",
    "lint": "npm run lint:ts & npm run format:ts",
    "lint:ts": "tslint --fix -t stylish -c tslint.json -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --collectCoverage",
    "format:ts": "prettier --parser typescript --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.1",
    "@nestjs/core": "^5.4.1",
    "@nestjs/websockets": "5.7.3",
    "@types/socket.io": "2.1.2",
    "axios": "^0.18.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.3.3",
    "winston": "^3.1.0",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.11",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.4.1",
    "uuid": "3.3.2"
  },
  "repository": {
    "type": "git"
  }
}
